/** ============================================================================
    Variables - custom per theme
============================================================================ **/
:root {
    --color-primary: #f64899;
    --color-primary--light: #fce4ec;
    --color-secondary: #382757;
    --text-color-primary: #382757;
    --text-color-accent: #f64899;

    --text-demi: '';
    --text-medium: '';
    --text-non-pro: '';
    --text-bold: '';
    --text-book: '';
}

